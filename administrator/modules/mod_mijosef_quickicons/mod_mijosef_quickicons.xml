<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5" client="administrator" method="upgrade">
	<name>MijoSEF - Quick Icons</name>
	<author>Mijosoft LLC</author>
	<creationDate>August 2012</creationDate>
	<copyright>(C) 2009-2012 Mijosoft LLC</copyright>
	<license>GNU/GPL http://www.gnu.org/copyleft/gpl.html</license>
	<authorEmail>info@mijosoft.com</authorEmail>
	<authorUrl>www.mijosoft.com</authorUrl>
	<version>1.1.0</version>
	<description>This module shows MijoSEF Quick Icons on the Admin area Home Page. Publish in "icon" position.</description>
	<files>
		<filename module="mod_mijosef_quickicons">mod_mijosef_quickicons.php</filename>
		<filename>index.html</filename>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB.mod_mijosef_quickicons.ini</language>
	</languages>
	<params>
		<param name="mijosef_version" type="radio" default="1" label="Version Status" description="Show Version Status button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_configuration" type="radio" default="0" label="Configuration" description="Show Configuration button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_extensions" type="radio" default="0" label="Extensions" description="Show Extensions button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_urls" type="radio" default="1" label="URLs" description="Show URLs button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_metadata" type="radio" default="1" label="Metadata" description="Show Metadata button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_sitemap" type="radio" default="1" label="Sitemap" description="Show Sitemap button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_tags" type="radio" default="1" label="Tags" description="Show Tags button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_ilinks" type="radio" default="0" label="Internal Links" description="Show Internal Links button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="mijosef_bookmarks" type="radio" default="0" label="Social Bookmarks" description="Show Social Bookmarks button.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
</install>